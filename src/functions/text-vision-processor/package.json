{
  "name": "text-vision-processor",
  "version": "1.0.0",
  "description": "Cloud Function for calling Vision API on Cloud Storage documents",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -f *.js *.js.map *.d.ts && rm -rf dist/",
    "test": "jest --config=../../../jest.config.js --testPathPatterns=text-vision-processor --logHeapUsage",
    "test:coverage": "cd ../../.. && jest --config=jest.config.js --coverage --testPathPatterns=text-vision-processor --collectCoverageFrom='src/functions/text-vision-processor/**/*.{ts,tsx}' --collectCoverageFrom='!src/functions/text-vision-processor/**/*.test.{ts,tsx}' --collectCoverageFrom='!src/functions/text-vision-processor/**/*.spec.{ts,tsx}' --logHeapUsage",
    "lint": "eslint --config=../../../eslint.config.js --fix",
    "format": "prettier --write '**/*.ts'"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^4.0.1",
    "@google-cloud/vision": "^5.3.4",
    "@google-cloud/storage": "^7.18.0",
    "@google/events": "^5.4.0",
    "autonyan-shared": "file:../../shared"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
